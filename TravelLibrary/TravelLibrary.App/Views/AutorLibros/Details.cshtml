@model TravelLibrary.App.Entities.AutorLibro

@{
    ViewData["Title"] = "Details";
}

<h2 class="alert alert-success">DETALLES</h2>

<div>
    <h4 class="alert alert-info">AUTOR Y LIBROS</h4>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            Nombres
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Autores.Nombre)
        </dd>
        <dt class="col-sm-2">
            Apellidos
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Autores.Apellido)
        </dd>
        <dt class="col-sm-2">
            Titulo
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Libros.Titulo)
        </dd>
        <dt class="col-sm-2">
            Sinopsis
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Libros.Sinopsis)
        </dd>
    </dl>
</div>
<div>
    <a asp-action="Index" class="btn btn-success">Regresar</a>
</div>